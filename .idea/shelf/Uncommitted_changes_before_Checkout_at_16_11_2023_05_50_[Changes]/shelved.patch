+function showContent(contentType: string) {
+    const allContent = document.querySelectorAll<HTMLElement>("#links span");
+    for (const content of Array.from(allContent)) {
+        content.style.display = "none"; // Verstecke alle Inhalte
+    }
+
+    const selectedContent = document.querySelector<HTMLElement>(`#${contentType}`);
+    if (selectedContent) {
+        selectedContent.style.display = "inline"; // Zeige ausgewählten Inhalt
+    }
+}
